{
  "extends": [
    "plugin:team-vals/recommended",
    "plugin:team-vals/react",
    "plugin:team-vals/tailwindcss",
    "plugin:team-vals/prettier"
  ],
  "parserOptions": {
    "project": "tsconfig.json",
    "tsconfigRootDir": "./"
  },
  "rules": {
    "tailwindcss/no-custom-classname": "off",
    "react/jsx-max-depth": [
      2,
      {
        "max": 10
      }
    ]
  },
  "ignorePatterns": [
    "gatsby-config.js"
  ]
}